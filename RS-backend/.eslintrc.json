{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "tsconfig.json"
  },
  "rules": {
    "constructor-super": 2,
    "for-direction": 2,
    "getter-return": 2,
    "no-async-promise-executor": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-import-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-loss-of-precision": 2,
    "no-misleading-character-class": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-symbol": 2,
    "no-nonoctal-decimal-escape": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-setter-return": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unsafe-optional-chaining": 2,
    "no-unused-labels": 2,
    "no-useless-backreference": 2,
    "no-useless-catch": 2,
    "no-useless-escape": 2,
    "no-with": 2,
    "require-yield": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty-function": 2,
    "camelcase": 2,
    "arrow-spacing": 2,
    "array-bracket-spacing": 2,
    "block-scoped-var": 2,
    "default-case": 2,
    "default-case-last": 2,
    "default-param-last": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "global-require": 2,
    "handle-callback-err": 2,
    "indent": 2,
    "init-declarations": 2,
    "quotes": [
      2,
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "lines-around-comment": 2,
    "multiline-comment-style": 2,
    "newline-before-return": 2,
    "no-alert": 2,
    "no-await-in-loop": 2,
    "no-console": 2,
    "no-tabs": 2,
    "no-new-wrappers": 2,
    "no-new-require": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-undefined": 2,
    "no-undef-init": 2,
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": true
      }
    ],
    "no-throw-literal": 2,
    "no-unreachable-loop": 2,
    "no-unused-expressions": 2,
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-var": 2,
    "no-whitespace-before-property": 2,
    "no-extra-parens": 2,
    "no-extra-label": 2,
    "object-curly-spacing": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "require-await": 2,
    "rest-spread-spacing": 2,
    "semi": 2,
    "sort-imports": [
      2,
      {
        "ignoreDeclarationSort": true
      }
    ],
    "sort-keys": 2,
    "space-in-parens": 2
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ]
}
